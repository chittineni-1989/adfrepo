{
	"name": "Https",
	"properties": {
		"activities": [
			{
				"name": "Copy data1",
				"type": "Copy",
				"dependsOn": [],
				"policy": {
					"timeout": "7.00:00:00",
					"retry": 0,
					"retryIntervalInSeconds": 30,
					"secureOutput": false,
					"secureInput": false
				},
				"userProperties": [],
				"typeProperties": {
					"source": {
						"type": "RestSource",
						"httpRequestTimeout": "00:01:40",
						"requestInterval": "00.00:00:00.010",
						"requestMethod": "GET",
						"paginationRules": {
							"supportRFC5988": "true"
						}
					},
					"sink": {
						"type": "SqlServerSink",
						"writeBehavior": "insert",
						"sqlWriterUseTableLock": false,
						"tableOption": "autoCreate"
					},
					"enableStaging": false,
					"translator": {
						"type": "TabularTranslator",
						"mappings": [
							{
								"source": {
									"path": "$['actor']['id']"
								},
								"sink": {
									"name": "actor_id",
									"type": "Single"
								}
							},
							{
								"source": {
									"path": "$['actor']['login']"
								},
								"sink": {
									"name": "actor_login",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['actor']['gravatar_id']"
								},
								"sink": {
									"name": "actor_gravatar_id",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['actor']['url']"
								},
								"sink": {
									"name": "actor_url",
									"type": "String"
								}
							},
							{
								"source": {
									"path": "$['actor']['avatar_url']"
								},
								"sink": {
									"name": "actor_avatar_url",
									"type": "String"
								}
							}
						],
						"collectionReference": "",
						"mapComplexValuesToString": true
					}
				},
				"inputs": [
					{
						"referenceName": "HttpsJsonRead",
						"type": "DatasetReference"
					}
				],
				"outputs": [
					{
						"referenceName": "SqlServerTable3",
						"type": "DatasetReference"
					}
				]
			}
		],
		"folder": {
			"name": "ReadingJsonthroughHttps"
		},
		"annotations": []
	}
}